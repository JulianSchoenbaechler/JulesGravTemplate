<div class="grid-element-1-1 grid-element-m-1-2">
  <a href="{{ project.link }}">
    <figure class="gallery">
      <img src="{{ page.media[project.image].url }}" alt="{{ project.title|e }}">
      <figcaption>
        <h5>{{ project.title|e }}</h5>
        <h6>{{ project.description|e }}</h6>
      </figcaption>
    </figure>
  </a>
</div>
